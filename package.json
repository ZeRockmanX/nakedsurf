{
  "name": "nakedsurf",
  "version": "1.0.0",
  "description": "A lightwight kit package base on KOA framework;",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "better-npm-run start",
    "deploy:preview": "better-npm-run deploy:prview",
    "deploy:prod": "better-npm-run deploy:prod"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "ENABLE_ENV": "dev"
      }
    },
    "deploy:prview": {
      "command": "babel-node index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "ENABLE_ENV": "preview"
      }
    },
    "deploy:prod": {
      "command": "babel-node index.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "ENABLE_ENV": "online"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leeleeEcho/nakedsurf.git"
  },
  "keywords": [
    "kit",
    "koa",
    "package"
  ],
  "author": "leelee.echo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leeleeEcho/nakedsurf/issues"
  },
  "homepage": "https://github.com/leeleeEcho/nakedsurf#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "bluebird": "^3.5.0",
    "co": "^4.6.0",
    "debug": "^2.2.0",
    "eval": "^0.1.1",
    "fs-promise": "^0.5.0",
    "handlebars": "^4.0.5",
    "ioredis": "^1.15.1",
    "ip": "^1.1.5",
    "joi": "^8.0.4",
    "js-md5": "^0.6.0",
    "jsonwebtoken": "^7.1.9",
    "koa": "^1.1.2",
    "koa-better-vhost": "0.0.1",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-json": "^1.1.1",
    "koa-jwt": "^1.2.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^5.4.0",
    "koa-session": "^3.3.1",
    "koa-static": "^2.0.0",
    "koa-unless": "^0.0.1",
    "lodash": "^4.17.4",
    "msgpack-lite": "^0.1.17",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "request": "^2.70.0",
    "sails-mongo": "^0.12.0",
    "sails-mysql": "^0.12.0",
    "thunkify": "^2.1.2",
    "validator": "^5.1.0",
    "waterline": "^0.12.1",
    "waterline-joi": "^1.0.12",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "better-npm-run": "^0.1.0",
    "concurrently": "^3.5.0",
    "debug": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rimraf": "^0.2.0",
    "gulp-util": "^3.0.7",
    "run-sequence": "^1.1.5",
    "sails-memory": "^0.10.6"
  }
}
